    xmlns:items="clr-namespace:BogheApp.Items"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="522" Width="347">
    
    <ctrl:BaseScreen.Resources>
        <DataTemplate x:Key="DataTemplateHistoryAVCallEvent">
            <items:ItemHistoryAVCallEvent Value="{Binding Path=.}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></items:ItemHistoryAVCallEvent>
        </DataTemplate>
    </ctrl:BaseScreen.Resources>
    
    <Grid>
        <ListBox Margin="0,34,0,0" Name="listBox" />
    </Grid>
</ctrl:BaseScreen>
